
<div>
    <!-- Links -->
    <div class="col-md-12 form-group label-floating" ng-repeat="rss in Rss.rssToSave">
        <div class="input-group" style="width:100%">
            <label class="control-label" for="ms-form-url-publicity" style="top:-30px;font-size: 12px;" ng-bind="'Link ' + getLanguageById(rss.CodigoIdioma)"></label>
            <div class="col-md-2" style="padding:5px 0px 0px 0px;">
                <oi-select oi-options="item.value as item.name for item in lists.https track by $index"
                            ng-model="rss.http">
                </oi-select>
            </div>
            <div class="col-md-9">
                <input type="text" class="form-control" placeholder="" ng-model="rss.UrlFeed" />
            </div>
            <div class="col-md-1">
                <a ng-if="rss.Consecutivo !== 0" ng-click="ToDelete.assign(rss,Rss.delete)" data-toggle="modal" data-target="#alertWarning" style="margin:0" class="btn-circle">
                    <i class="zmdi zmdi-close color-danger"></i>
                </a>
            </div>
        </div>
    </div>
</div>
<!-- Buttons save & cancel-->
<div class="col-md-12" style="padding-left:30px;padding-right:50px;">
    <div class="btn-group btn-group-justified btn-group-raised">
        <a href="#" class="btn btn-success" ng-click="Rss.save()">
            <img ng-show="Rss.isBusy" src="~/Content/assets/img/ring.gif" width="25" />
            {{Rss.isBusy ? ('BTN_SAVING' | translate) : ('BTN_SAVE' | translate)}}
        </a>
        <a href="#" class="btn btn-danger" ng-click="Rss.viewFormRss = false" translate="BTN_CANCEL"></a>
    </div>
</div>